# Discord Bot Configuration
# Replace with your actual bot token from Discord Developer Portal
DISCORD_TOKEN=your_discord_bot_token_here

# Bot Application ID (needed for slash commands)
CLIENT_ID=your_application_id_here

# Bot Settings
PREFIX=!

# Your Discord user ID (for owner privileges)
OWNER_ID=

# Admin Users (comma-separated Discord user IDs)
ADMIN_USERS=

# API Settings
API_PORT=3000 